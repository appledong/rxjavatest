<?xml version="1.0" encoding="utf-8"?>
<manifest
    package="com.dong.rxjavatest"
    xmlns:android="http://schemas.android.com/apk/res/android">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".activity.MainActivity"
            android:screenOrientation="portrait">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        <activity android:name=".activity.change.FlatMapAndFromOperationActivity"
                  android:screenOrientation="portrait"/>
        <activity android:name=".activity.create.LogicObservableoperationActivity"
                  android:screenOrientation="portrait"/>
        <activity android:name=".activity.create.CalculateObservableOperationActivity"
                  android:screenOrientation="portrait"/>
        <activity android:name=".activity.thread.SchedulerActivity"
                  android:screenOrientation="portrait"/>
        <activity android:name=".activity.change.BufferActivity"
                  android:screenOrientation="portrait"/>
        <activity android:name=".activity.change.GroupByActivity"
                  android:screenOrientation="portrait"/>
        <activity android:name=".activity.CommonActivity"
                  android:screenOrientation="portrait"/>
    </application>

</manifest>
